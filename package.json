{
  "name": "aiventory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:performance": "node src/tests/performance/run-performance-tests.js",
    "test:load": "node src/tests/performance/load-testing.js",
    "test:security": "node src/tests/security/security-tests.js",
    "test:accessibility": "playwright test src/tests/accessibility/**/*.spec.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:enhanced": "playwright test --config=playwright-enhanced.config.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:comprehensive": "node src/tests/run-comprehensive-tests.js",
    "test:comprehensive:95-coverage": "node src/tests/run-comprehensive-tests.js",
    "test:coverage:enhance": "node src/tests/coverage/enhance-coverage.js",
    "test:comprehensive:critical": "node src/tests/run-comprehensive-tests.js --suite=critical",
    "test:comprehensive:important": "node src/tests/run-comprehensive-tests.js --suite=important",
    "test:comprehensive:supplementary": "node src/tests/run-comprehensive-tests.js --suite=supplementary",
    "test:comprehensive:all": "node src/tests/run-comprehensive-tests.js --suite=all",
    "test:comprehensive:headed": "node src/tests/run-comprehensive-tests.js --headed",
    "test:comprehensive:debug": "node src/tests/run-comprehensive-tests.js --debug",
    "test:100-coverage-database": "npx playwright test src/tests/database-100-percent.spec.ts",
    "test:100-coverage-frontend": "npx playwright test src/tests/frontend-100-percent.spec.ts",
    "test:100-coverage-ai": "npx playwright test src/tests/ai-100-percent.spec.ts",
    "test:100-coverage-business": "npx playwright test src/tests/business-logic-100-percent.spec.ts",
    "test:100-coverage-integration": "npx playwright test src/tests/integration-100-percent.spec.ts",
    "test:100-coverage-security": "npx playwright test src/tests/security-100-percent.spec.ts",
    "test:100-coverage-master": "npx playwright test src/tests/100-percent-coverage-master.spec.ts",
    "test:100-coverage-all": "npm run test:100-coverage-database && npm run test:100-coverage-frontend && npm run test:100-coverage-ai && npm run test:100-coverage-business && npm run test:100-coverage-integration && npm run test:100-coverage-security",
    "test:complete-suite": "npm run test:comprehensive && npm run test:e2e && npm run test:100-coverage-all"
  },
